<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.clicker1">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".PayPalLinkActivity"></activity>
        <activity android:name=".AdActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".menueActivity"></activity>
        <activity android:name=".AgbActivity"></activity>
        <activity android:name=".SignOutActivity"></activity>
        <activity android:name=".PayOutActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-5076771642860864~3907873771"
            />

    </application>

</manifest>